{
  "name": "monday-structurizr-integration",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run stop && concurrently \"nodemon\" \"npm run expose\"",
    "start:dev": "npm run build && node ./dist/app.js",
    "start:prod": "node ./dist/app.js",
    "expose": "ngrok http 8302",
    "stop": "kill-port 8302 && kill-port 404 9 && kill-port 4040",
    "build": "tsc"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "src/"
    ],
    "exec": "npm run start:dev",
    "ext": "ts"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "monday-sdk-js": "^0.1.1",
    "mongoose": "^5.10.16",
    "proxyquire": "^2.0.1",
    "querystring": "^0.2.0",
    "structurizr-typescript": "^1.0.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.1",
    "concurrently": "^5.2.0",
    "cross-port-killer": "^1.2.1",
    "debug": "~2.6.9",
    "eslint": "^v6.8.0",
    "ngrok": "^3.3.0",
    "nodemon": "^2.0.2",
    "nyc": "^12.0.2",
    "typescript": "^4.0.5"
  },
  "author": {
    "name": "Dev Agrawal",
    "email": "agrawadv@mail.uc.edu"
  },
  "contributors": [
    {
      "name": "Anmol Gupta",
      "email": "gupta.28@iitj.ac.in"
    }
  ]
}
